        </xs:complexType>
    </xs:element>

    <xs:element name="GetComptesByStatusTypeResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
                <xs:element name="comptes" type="tns:CompteSoapInfo" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Get Solde -->
    <xs:element name="GetSoldeRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="rib" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetSoldeResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="serviceStatus" type="tns:serviceStatus"/>
